<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kc.shiptransport">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mvp.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".mvp.main.MainActivity"/>
        <activity android:name=".mvp.plan.PlanActivity"/>
        <activity android:name=".mvp.plansetting.PlanSetActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.shipselect.ShipSelectActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.supply.SupplyActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.acceptance.AcceptanceActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.supplydetail.SupplyDetailActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.acceptancedetail.AcceptanceDetailActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.voyageinfo.VoyageInfoActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.voyagedetail.VoyageDetailActivity" android:launchMode="singleTask"/>
        <activity android:name=".view.actiivty.InputActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.scanner.ScannerActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.scannerdetail.ScannerDetailActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.amount.AmountActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.amountdetail.AmountDetailActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.sample.SampleActivity" android:launchMode="singleTask"/>
        <activity android:name=".mvp.sampledetail.SampleDetailActivity" android:launchMode="singleTask"/>
    </application>

</manifest>